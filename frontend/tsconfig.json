{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "Node",
    "strict": true
  },
  "include": ["./src/Types/**.d.ts"]
}
